<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	
	<package name="default" extends="struts-default">
	
		<action name="logar" class="actions.LoginAction" method="logar">
			<result name="success">/home.jsp</result>
			<result name="input">/index.jsp</result>
		</action>
		
		<action name="saveOrUpdateApartamento" class="actions.ApartamentoAction" method="saveOrUpdate">
			<result name="success">/cadastroApartamento.jsp</result>
		</action>
		
		<action name="preparePessoa" class="actions.PessoaAction" method="execute">
			<result name="success">/cadastroPessoa.jsp</result>
		</action>
		
		
		
	</package>	
	
</struts>
